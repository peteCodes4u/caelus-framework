{
  "name": "caelus-framework",
  "version": "1.0.0",
  "description": "MERN Stack Framework with GraphQL.",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm run dev\"",
    "install": "cd client && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peteCodes4u/caelus-framework.git"
  },
  "keywords": [
    "Mongo",
    "Node",
    "React",
    "Express",
    "GraphQL",
    "JavaScript"
  ],
  "author": "peteCodes4u",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peteCodes4u/caelus-framework/issues"
  },
  "homepage": "https://github.com/peteCodes4u/caelus-framework#readme",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
